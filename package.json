{
  "name": "fayredge-dashboard",
  "version": "1.0.0",
  "description": "Full stack dashboard for Fayredge real estate",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "npm run server --prefix server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && npm install --prefix client && npm install --prefix server",
    "seed-db": "npm run data:import --prefix server"
  },
  "keywords": [
    "react",
    "node",
    "express",
    "mongodb",
    "dashboard"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.1.0"
  }
}